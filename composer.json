{
    "require": {
        "symfony/symfony": "^3.0",
        "sensio/framework-extra-bundle": "^3.0",
        "twig/twig": "1.*",
        "monolog/monolog": "1.*",
        "symfony/routing": "3.*",
        "symfony/class-loader": "@stable",
        "symfony/yaml": "@stable",
        "nojacko/email-validator": "~1.0",
        "league/geotools": "@stable",
        "swiftmailer/swiftmailer": "@stable",
        "phpfastcache/phpfastcache": "3.0.23",
        "twig/extensions": "^1.3"
    },
    "autoload": {
        "files": ["src/functions.php"],
        "classmap": ["inc/", "plugin/"],
        "exclude-from-classmap": ["inc/core/"],
        "psr-4": {
            "Runalyze\\": "inc/core/",
            "Runalyze\\Bundle\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    }
}
